@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
	Layout = null;
	    var pageTitle = !string.IsNullOrEmpty(ViewBag.Title) ? $"{ViewBag.Title} | Onatrix - Financial Solutions for Your Business" : "Onatrix - Financial Solutions for Your Business";
        var logoImg = Model?.Value<IPublishedContent>("logoImg")?.Url() ?? "";
}

<!DOCTYPE html>
<html lang="sv">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
   <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
   <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">


    
    <title>@pageTitle </title>
  
    <link rel="stylesheet" href="css/main.css">

    
</head>

<body> 
<div class="wrapper">
    @await Html.PartialAsync("partials/static/socialmedia.cshtml")
    
    @await Html.PartialAsync("partials/static/header.cshtml")

    <main>
        @RenderBody()
    </main>

    @await Html.PartialAsync("partials/static/footer.cshtml")
</div>

		
	



<script src="/script/main.js" > </script>


</body>